  "license": "MIT",
  "author": "Theo",
  "scripts": {
    "docs:build": "pnpm -F=docs build",
    "docs:dev": "pnpm -F=docs dev",
    "docs:preview": "pnpm -F=docs preview",
    "clean": "pnpm clean:pnpm && pnpm clean:nm && pnpm clean:vp",
    "clean:nm": "rm -rf */node_modules node_modules pnpm-lock.yaml",
    "clean:pnpm": "rm -rf pnpm store prune && pnpm cache delete '*'",
    "clean:vp": "rm -rf docs/.vitepress/{cache,dist}",
    "iall": "pnpm -r install",
    "upall": "pnpm -r update --latest"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@commitlint/types": "^20.4.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "commitizen": "^4.3.1",
    "cz-vinyl": "^2.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.1",
    "prettier-plugin-css-order": "^2.2.0",
    "prettier-plugin-jsdoc": "^1.8.0",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-sort-json": "^4.2.0",
    "prettier-plugin-vitepress": "^0.0.10",
    "simple-git-hooks": "^2.13.1"
  },
  "packageManager": "pnpm@10.28.2"
}